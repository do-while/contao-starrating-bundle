{
  "name": "do-while/contao-starrating-bundle",
  "description": "Eine Sternebewertung die per InsertTag an beliebiger Stelle positioniert wird und ab dem ersten Vote erst im Backend in einer Bewertungsverwaltung erscheint.",
  "keywords": ["Bewertung","Sterne-Bewertung", "star", "rating","simple"],
  "type": "contao-module",
  "homepage": "https://github.com/do-while/contao-starrating-bundle",
  "license": "LGPL-3.0-or-later",
  "authors":[
    {
      "name":"Sven Rhinow",
      "homepage":"https://www.sr-tag.de",
      "role":"Developer"
    },
    {
      "name":"Softleister - Hagen Klemp",
      "homepage":"https://www.softleister.de",
      "role":"Developer"
    }
  ],
  "support":{
    "email": "info@softleister.de",
    "source":"https://github.com/do-while/contao-starrating-bundle"
  },
  "require": {
    "php": ">=7.0",
    "contao/core-bundle": "~3.5 || ~4.4",
    "contao-community-alliance/composer-plugin":"~2.4 || ~3.0"
  },
  "conflict": {
    "srhinow/contao-starrating-bundle": ">=0.0.1"
  },
  "autoload": {
    "psr-4": {
      "Softleister\\ContaoStarRatingBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Softleister\\ContaoStarRatingBundle\\ContaoManager\\Plugin"
  }
}
